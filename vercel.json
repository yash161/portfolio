{
    "version": 2,
    "builds": [
      {
        "src": "/public/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "/public/style.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  